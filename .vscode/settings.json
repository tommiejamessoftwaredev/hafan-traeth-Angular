{
    "azureFunctions.deploySubpath": "api/bin/Release/net8.0/publish",
    "azureFunctions.projectLanguage": "C#",
    "azureFunctions.projectRuntime": "~4",
    "debug.internalConsoleOptions": "neverOpen",
    "azureFunctions.preDeployTask": "publish (functions)",
    "dotnet.defaultSolution": "disable",
    "dotnet-test-explorer.testProjectPath": "**/*Tests.csproj",
    "dotnet-test-explorer.useTreeView": true,
    "dotnet-test-explorer.showCodeLens": true,
    "dotnet-test-explorer.codeLensFailed": "❌",
    "dotnet-test-explorer.codeLensPassed": "✅",
    "dotnet-test-explorer.codeLensSkipped": "⚠️",
    "jestTestExplorer.jestCommand": "npm test",
    "jestTestExplorer.rootPath": "./frontend/hafan-traeth",
    "jestTestExplorer.testNamePattern": ".*\\.spec\\.ts$",
    "jestTestExplorer.showCoverageOnLoad": false,
    "testExplorer.useNativeTesting": true,
    "testExplorer.gutterDecoration": true,
    "testExplorer.codeLens": true,
    "testExplorer.showCollapseButton": true,
    "testExplorer.showExpandButton": true,
    "testExplorer.showOnRun": true
}